{
  "name": "react-forms",
  "version": "1.0.0",
  "main": "dist/react-forms.js",
  "module": "src/index.js",
  "author": {
    "email": "alesmenzel@gmail.com",
    "name": "AleÅ¡ Menzel"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "run-s clean build:dev",
    "build:min": "run-s clean build:prod",
    "build:dev": "cross-env NODE_ENV=development webpack --output dist/react-form.js",
    "build:prod": "cross-env NODE_ENV=production webpack --output dist/react-form.min.js",
    "build:watch": "npm run build -- --watch",
    "test": "npm run lint && npm run test:suite",
    "test:suite": "cross-env NODE_ENV=test mocha --require ./test/setup.js --require babel-polyfill --compilers js:babel-core/register ./src/**/*.spec.js",
    "test:suite:watch": "npm run test:suite -- --watch",
    "coverage": "cross-env NODE_ENV=test nyc npm test",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"./**/*.js\"",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:AlesMenzel/react-forms.git"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "3.1.0",
    "isemail": "3.0.0",
    "prop-types": "15.5.0",
    "url": "0.11.0"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.1",
    "babel-plugin-syntax-export-extensions": "6.13.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.1.0",
    "cross-env": "5.0.2",
    "dirty-chai": "2.0.1",
    "enzyme": "3.3.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.4.0",
    "husky": "0.15.0-rc.13",
    "jsdom": "11.1.0",
    "lint-staged": "7.0.0",
    "mocha": "5.0.5",
    "npm-run-all": "4.1.2",
    "nyc": "11.1.0",
    "prettier": "1.11.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "webpack": "4.2.0",
    "webpack-cli": "2.0.13"
  },
  "peerDependencies": {
    "react": "16.0.0",
    "react-dom": "16.0.0"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged",
      "prepush": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
